<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

 <mapper namespace="com.javatiaocao.myblog.mapper.FeedBackMapper"><!-- 命名空间，用来找到对应的xml文件 -->

  <select id="getAllFeedback" resultType="com.javatiaocao.myblog.model.FeedBack">
      select * from feedback order by id desc
  </select>

    <select id="getAllPrivateWord" resultType="com.javatiaocao.myblog.model.PrivateWord">
        select * from privateword order by publisherDate desc
    </select>
</mapper>
